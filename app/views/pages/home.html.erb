<h1>SUP THUGS.</h1>









<table>
  <th class="blue-text bold underlined">USERS</th>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Age</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name%></td>
      <td><%= user.email%></td>
      <td><%= user.age%></td>
    </tr>
  <% end %>
</table>

<table>
<th class="blue-text bold underlined">CATS</th>
 <tr>
   <th>Name</th>
 </tr>
  <% @cats.each do |cat| %>
    <tr>
      <td><%= cat.name%></td>
    </tr>
  <% end %>
</table>

<table>
  <th class="blue-text bold underlined">TODO</th>
  <tr>
    <th>Task</th>
    <th>Finished?</th>
  </tr>
  <% @todos.each do |todo| %>
    <tr>
      <td><%= todo.task%></td>
      <td><%= todo.finished%></td>
    </tr>
  <% end %>
</table>